{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": "arn:aws:logs:*:*:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "rds:DescribeDBSnapshots",
                "rds:DeleteDBSnapshot",
                "rds:CopyDBSnapshot",
                "rds:ModifyDBSnapshotAttribute",
                "SNS:Publish"
            ],
            "Resource": "*"
        }
    ]
}
